{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"jsx": "react-native",
		"lib": ["dom", "esnext"],
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": false
	}
}
